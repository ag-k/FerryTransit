{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "npm run dev:setup && cp .env.local .env && nuxt dev",
    "dev:setup": "node scripts/dev-setup.mjs",
    "dev:full": "concurrently \"npm run firebase:emulators:with-admin\" \"npm run dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "lint": "eslint .",
    "typecheck": "nuxt typecheck",
    "firebase:emulators": "firebase emulators:start",
    "firebase:emulators:with-admin": "node scripts/start-emulators-with-admin.mjs",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:hosting": "firebase deploy --only hosting",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "deploy": "npm run build && firebase deploy",
    "fetch:routes": "node scripts/fetch-routes.mjs",
    "fetch:routes:online": "node scripts/fetch-online-routes.mjs",
    "cap:build": "CAPACITOR_BUILD=true npm run generate && npx cap sync",
    "cap:ios": "npm run cap:build && npx cap open ios",
    "cap:android": "npm run cap:build && npx cap open android",
    "build-prod": "run-s build-prod:firebase build-prod:build",
    "build-prod:firebase": "firebase use prod",
    "build-prod:build": "NODE_ENV=production nuxt build",
    "build-dev": "run-s build-dev:firebase build-dev:build",
    "build-dev:firebase": "firebase use dev",
    "build-dev:build": "NODE_ENV=development nuxt build",
    "deploy-prod": "run-s build-prod deploy",
    "deploy-dev": "run-s build-dev deploy",
    "build-app-dev": "cp .env.development .env && CAPACITOR_BUILD=true npm run generate && npx cap copy",
    "build-app-prod": "cp .env.production .env && CAPACITOR_BUILD=true npm run generate && npx cap copy"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/assets": "^3.0.5",
    "@capacitor/core": "^7.4.2",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/icon": "^1.15.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@pinia/nuxt": "^0.11.2",
    "@types/google.maps": "^3.58.1",
    "@types/marked": "^5.0.2",
    "@types/papaparse": "^5.3.16",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.4.0",
    "marked": "^16.1.1",
    "nuxt": "^3.18.0",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.2",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^20.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^4.1.11",
    "vitest": "^3.2.4"
  },
  "optionalDependencies": {
    "@oxc-parser/binding-darwin-arm64": "^0.73.2"
  }
}
