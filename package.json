{
  "name": "nuxt-app",
  "version": "2.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "npm run dev:setup && nuxt dev --dotenv .env.local",
    "opencode": "bash scripts/start-opencode.sh",
    "dev:setup": "node scripts/dev-setup.mjs",
    "dev:full": "concurrently -k --kill-others-on-fail --kill-signal SIGINT --names \"emulators,nuxt\" \"npm run firebase:emulators:with-data\" \"npm run dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest --run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --run",
    "test:api": "vitest run src/test/unit/api/",
    "test:api:watch": "vitest src/test/unit/api/",
    "test:integration": "vitest run src/test/integration/",
    "test:integration:watch": "vitest src/test/integration/",
    "test:helpers": "vitest run src/test/unit/api/helpers.test.ts",
    "test:all": "vitest run src/test/unit/api/ src/test/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:appstore-screenshots": "APPSTORE_STATIC_ROOT=.output/public APPSTORE_SCREENSHOT_DIR=output/appstore-screenshots/ios-6.7-ja playwright test -c playwright.appstore.config.ts src/tests/e2e/appstore-screenshots.spec.ts --workers=1",
    "lint": "eslint src",
    "typecheck": "nuxt typecheck",
    "firebase:emulators": "firebase emulators:start",
    "firebase:emulators:with-admin": "node scripts/start-emulators-with-admin.mjs",
    "firebase:emulators:with-data": "node scripts/start-emulators-with-data.mjs",
    "firebase:emulators:export": "node scripts/export-emulator-data.mjs",
    "firebase:deploy": "firebase deploy --project prod",
    "firebase:deploy:hosting": "firebase use prod && firebase deploy --only hosting",
    "firebase:deploy:functions": "firebase deploy --only functions --project prod",
    "deploy": "run-s build-prod deploy:only",
    "deploy:only": "firebase deploy --project prod",
    "deploy:only:dev": "firebase deploy --project dev",
    "fetch:routes": "node scripts/fetch-routes.mjs",
    "fetch:routes:online": "node scripts/fetch-online-routes.mjs",
    "cap:ios": "node scripts/cap-build.mjs ios --open",
    "cap:android": "node scripts/cap-build.mjs android --open",
    "cap:ios:build": "node scripts/cap-build.mjs ios",
    "cap:android:build": "node scripts/cap-build.mjs android",
    "cap:ios:appstore-screenshots": "node scripts/capture-ios-simulator-screenshots.mjs",
    "build-prod": "run-s build-prod:firebase build-prod:build",
    "build-prod:firebase": "firebase use prod",
    "build-prod:build": "NODE_ENV=production nuxt generate --dotenv .env.production",
    "build-dev": "run-s build-dev:firebase build-dev:build",
    "build-dev:firebase": "firebase use dev",
    "build-dev:build": "NODE_ENV=development nuxt generate --dotenv .env.development",
    "deploy-prod": "run-s build-prod deploy:only",
    "deploy-prod:hosting": "run-s build-prod:build firebase:deploy:hosting",
    "deploy-dev": "run-s build-dev deploy:only:dev",
    "build-app-dev": "cp .env.development .env && CAPACITOR_BUILD=true npm run generate && npx cap copy",
    "functions:deploy:script": "bash scripts/deploy-functions.sh"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/app": "^7.0.1",
    "@capacitor/core": "^7.4.2",
    "@capacitor/haptics": "^7.0.1",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/keyboard": "^7.0.1",
    "@capacitor/splash-screen": "^7.0.1",
    "@capacitor/status-bar": "^7.0.1",
    "@googlemaps/js-api-loader": "^1.16.10",
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/icon": "^1.15.0",
    "@nuxtjs/google-fonts": "^3.2.0",
    "@nuxtjs/i18n": "^9.5.6",
    "@pinia/nuxt": "^0.11.2",
    "@types/google.maps": "^3.58.1",
    "@types/marked": "^5.0.2",
    "@types/papaparse": "^5.3.16",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.13",
    "firebase": "^11.10.0",
    "firebase-admin": "^13.6.1",
    "firebase-functions": "^7.0.5",
    "marked": "^16.1.1",
    "nuxt": "^3.21.1",
    "papaparse": "^5.5.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.5.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^7.0.0-beta.0",
    "@pinia/testing": "^1.0.2",
    "@playwright/test": "^1.54.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^24.1.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "happy-dom": "^20.0.10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^4.1.11",
    "vite": "^7.3.1",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.4"
  },
  "optionalDependencies": {
    "@oxc-parser/binding-darwin-arm64": "0.102.0",
    "@oxc-parser/binding-darwin-x64": "0.102.0"
  },
  "overrides": {
    "@google-cloud/storage": "^7.19.0",
    "@isaacs/brace-expansion": "^5.0.1",
    "diff": "^8.0.3",
    "lodash": "^4.17.23",
    "minimatch": "^10.2.1",
    "nanotar": "^0.3.0",
    "qs": "^6.14.2"
  }
}
